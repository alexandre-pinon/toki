{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_PROFILE": "dev"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_PROFILE": "preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "APP_PROFILE": "prod"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
